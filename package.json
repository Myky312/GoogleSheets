    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "mathjs": "^13.2.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "router": "^1.3.8",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
